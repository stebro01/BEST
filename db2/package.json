{
  "name": "dbbest",
  "version": "1.0.0",
  "description": "dbBEST - Forschungsdatenbank",
  "productName": "dbBEST",
  "author": "ste@m1 <ste>",
  "private": true,
  "scripts": {
    "dev": "ELECTRON_DISABLE_SECURITY_WARNINGS=true quasar dev -m electron",
    "build": "quasar build -m electron",
    "build_sign": "electron-osx-sign ./dist/electron/dbNew-darwin-arm64/dbNew.app --type=development",
    "build_dmg": "electron-installer-dmg ./dist/electron/dbNew-darwin-arm64/dbNew.app dbBEST --out=./dist --overwrite",
    "build4win": "quasar build --mode electron --target win32 ",
    "icons": "icongenie generate -i ./public/icons/favicon.png",
    "doc": "jsdoc -r -c jsdoc.conf.json",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "electron-installer-dmg": "^4.0.0",
    "human-readable-ids": "^1.0.4",
    "jsrsasign": "^10.6.1",
    "quasar": "^2.6.0",
    "sqlite3": "^5.1.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app-webpack": "^3.0.0",
    "electron": "^23.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.5.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.5.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
