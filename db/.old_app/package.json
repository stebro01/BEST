{
  "name": "dbbest",
  "version": "0.0.1",
  "description": "sqliteDB and electron",
  "productName": "dbBEST",
  "author": "sb",
  "private": true,
  "scripts": {
    "dev": "ELECTRON_DISABLE_SECURITY_WARNINGS=true quasar dev -m electron -t mat",
    "build": "quasar build -m electron -t mat",
    "build_sign": "electron-osx-sign ./dist/electron/dbBEST-darwin-arm64/dbBEST.app --type=development",
    "build_dmg": "electron-installer-dmg ./dist/electron/dbBEST-darwin-arm64/dbBEST.app dbBEST --out=./dist --overwrite",
    "build4win": "quasar build --mode electron --target win32 ",
    "cy": "npm run test:e2e",
    "icons": "icongenie generate -i ./public/icons/favicon.png",
    "doc": "jsdoc -r -c jsdoc.conf.json",
    "manual": "sphinx-build -b html manual/ manual/_build/html",
    "manual_pdf": "sphinx-build -b pdf manual/ manual/_build/pdf",
    "manual_epub": "sphinx-build -b epub manual/ manual/_build/epub",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0"
  },
  "dependencies": {
    "@electron/osx-sign": "^1.0.4",
    "@quasar/extras": "^1.15.5",
    "core-js": "^3.26.0",
    "crypto-js": "^4.1.1",
    "fs": "0.0.1-security",
    "human-readable-ids": "^1.0.4",
    "jsencrypt": "^3.3.1",
    "jsrsasign": "^10.6.1",
    "path": "^0.12.7",
    "quasar": "^2.10.1",
    "sqlite3": "^5.1.2",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@quasar/app-webpack": "^3.6.2",
    "@quasar/quasar-app-extension-testing": "^2.0.4",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.2.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-beta.3",
    "@vue/compiler-sfc": "^3.2.41",
    "@vue/test-utils": "^2.2.0",
    "electron": "^15.5.7",
    "electron-builder": "^22.14.13",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^15.5.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.7.0",
    "jest": "^29.2.2",
    "jsdoc": "^4.0.0",
    "jsdoc-vuejs": "^4.0.0",
    "jsdom-global": "^3.0.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
