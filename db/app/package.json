{
  "name": "dbbest",
  "version": "1.0.0",
  "description": "dbBEST - Forschungsdatenbank",
  "productName": "dbBEST",
  "author": "ste@m1 <ste>",
  "private": true,
  "scripts": {
    "dev": "ELECTRON_DISABLE_SECURITY_WARNINGS=true quasar dev -m electron",
    "build": "quasar build -m electron",
    "build_sign": "electron-osx-sign ./dist/electron/dbBEST-darwin-arm64/dbBEST.app --type=development",
    "build_dmg": "electron-installer-dmg ./dist/electron/dbBEST-darwin-arm64/dbBEST.app dbBEST --out=./dist --overwrite",
    "build4win": "quasar build --mode electron --target win32 -b builder",
    "build4win2": "quasar build -m electron -T win32 -b packager -A ia32",
    "build4linux": "quasar build -m electron -T linux -b packager -A x64",
    "icons": "icongenie generate -i ./public/icons/favicon.png",
    "doc": "jsdoc -r -c jsdoc.conf.json",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "jest --updateSnapshot",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "appdmg": "^0.6.6",
    "core-js": "^3.6.5",
    "cql-execution": "^2.4.4",
    "crypto-js": "^4.1.1",
    "fs": "^0.0.1-security",
    "html2pdf.js": "^0.10.1",
    "human-readable-ids": "^1.0.4",
    "jsrsasign": "^10.6.1",
    "quasar": "^2.6.0",
    "sqlite3": "^5.1.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@electron/osx-sign": "^1.0.4",
    "@quasar/app-webpack": "^3.0.0",
    "@quasar/quasar-app-extension-testing": "^2.0.6",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.0.0-beta.11",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-beta.5",
    "@vue/test-utils": "^2.2.0",
    "cypress": "^12.2.0",
    "electron": "^23.0.0",
    "electron-builder": "^22.14.13",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^15.5.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "jest": "^29.2.2",
    "jsdoc": "^4.0.0",
    "jsdoc-vuejs": "^4.0.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.5.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
