{
  "name": "dbbest",
  "version": "1.0.0",
  "description": "dbBEST - Forschungsdatenbank",
  "productName": "dbBEST",
  "author": "ste <ste@mba>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "ELECTRON_DISABLE_SECURITY_WARNINGS=true quasar dev -m electron",
    "build": "quasar build -m electron",
    "build4win": "quasar build --mode electron --target win32 -b builder && name=\"dbBest_$(jq -r '.app.version' ./public/env.json)_win32.zip\" && zip -r ./dist/$name ./dist/electron/packaged/",
    "build4win2": "quasar build -m electron -T win32 -b packager -A ia32",
    "build4linux": "quasar build -m electron -T linux -b packager -A x64",
    "icons": "icongenie generate -i ./public/icons/favicon.png",
    "doc": "jsdoc -r -c jsdoc.conf.json",
    "test:unit": "vitest",
    "test:jest": "jest"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.12",
    "axios": "^1.7.2",
    "cql-execution": "^3.1.0",
    "crypto-js": "^4.2.0",
    "dateformat": "^5.0.3",
    "html2pdf.js": "^0.10.2",
    "human-readable-ids": "^1.0.4",
    "jsencrypt": "^3.3.2",
    "jsrsasign": "^11.1.0",
    "quasar": "^2.16.8",
    "sqlite3": "^5.1.4",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "vuedraggable": "^2.24.3",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@electron/packager": "^18.3.2",
    "@quasar/app-vite": "^1.9.4",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^29.7.0",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": "^22 || ^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
